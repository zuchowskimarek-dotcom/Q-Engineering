            "rawQuery": true,
            "rawSql": "SELECT\n  $__timeGroupAlias(timestamp,$__interval),\n    p.name AS metric,\n      SUM(m.churn) AS \"churn\"\n      FROM \"ProjectMetric\" m\n      JOIN \"Project\" p ON m.\"projectId\" = p.id\n      JOIN \"TeamProject\" tp ON p.id = tp.\"projectId\"\n      JOIN \"TeamMembership\" tm ON tp.\"teamId\" = tm.\"teamId\"\n      WHERE\n        $__timeFilter(timestamp) AND   (p.id IN ($project) OR 'all' IN ($project)) AND   (tp.\"teamId\" IN ($team) OR 'all' IN ($team)) AND   (tm.\"personId\" IN ($member) OR 'all' IN ($member))\n        GROUP BY 1, 2\n        ORDER BY 1, 2",
